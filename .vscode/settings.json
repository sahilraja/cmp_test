{
    "editor.formatOnSave": false,
    "editor.formatOnPaste": false,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.zip": true
    },
    "files.exclude": {
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/*.js.map": true
    },
    "cSpell.words": [
        "docid",
        "parentid"
    ]
}